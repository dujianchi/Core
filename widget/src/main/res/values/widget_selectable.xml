<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SelectableLayout">
        <attr name="du_selectableLayoutSelectCount" format="integer" />
        <attr name="du_selectableLayoutSelectedIndexes" format="string" />
        <attr name="du_selectableLayoutCancelable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SelectableView">
        <attr name="du_selectableViewLayoutId" format="reference" />
        <attr name="du_selectableViewIconWidth" format="dimension" />
        <attr name="du_selectableViewIconHeight" format="dimension" />
        <attr name="du_selectableViewIconSrc" format="reference" />
        <attr name="du_selectableViewIconTextSpacing" format="dimension" />
        <attr name="du_selectableViewTextSize" format="dimension" />
        <attr name="du_selectableViewTextColor" format="color" />
        <attr name="du_selectableViewText" format="string" />
    </declare-styleable>

    <item name="du_selectableViewTextId" type="id" />
    <item name="du_selectableViewIconId" type="id" />
</resources>